math_list=['가형','나형']
math_list=input('당신의 수학은 무슨형입니까?')
print('______________________________________________')
#mb=수학가형 점수 
if math_list==('가형'):
    mb=int(input('당신의 가형 수학점수를 입력하시오'))
    if (mb>=92):
        mb=(mb>=92)
        mb=1
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(92>mb>=85):
        mb=(92>mb>=85)
        mb=2
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(85>mb>=80):
        mb=(85>mb>=80)
        mb=3
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(80>mb>=70):
        mb=(80>mb>=70)
        mb=4
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(70>mb>=54):
        mb=(70>mb>=54)
        mb=5
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(54>mb>=36):
        mb=(54>mb>=36)
        mb=6
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(36>mb>=22):
        mb=(36>mb>=22)
        mb=7
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(22>mb>=13):
        mb=(22>mb>=13)
        mb=8
        print('당신의 수학등급은',mb,'등급입니다.')
    elif(13>mb):
        mb=(13>mb)
        mb=9
        print('당신의 수학등급은',mb,'등급입니다.')
    else:
        print('숫자로 입력해주십쇼')
    print('______________________________________________')
#mc=수학나형점수
elif math_list==('나형') :
    mc=int(input('당신의 나형 수학점수를 입력하시오'))
    if (84>=mc):
        mc=(84>=mc)
        mc=1
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(84>mc>=76):
        mc=(84>mc>=76)
        mc=2
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(76>mc>=65):
        mc=(76>mc>=65)
        mc=3
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(65>mc>=51):
        mc=(65>mc>=51)
        mc=4
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(51>mc>=35):
        mc=(51>mc>=35)
        mc=5
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(35>mc>=23):
        mc=(35>mc>=23)
        mc=6
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(23>mc>=16):
        mc=(23>mc>=16)
        mc=7
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(16>mc>=11):
        mc=(16>mc>=11)
        mc=8
        print('당신의 나형 수학등급은',mc,'입니다.')
    elif(11>mc):
        mc=(11>mc)
        mc=9
        print('당신의 나형 수학등급은',mc,'입니다.')
